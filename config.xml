<?xml version='1.0' encoding='utf-8'?>
<widget id="com.natisp.theprofessorbook" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>The Professor Book</name>
    <description>
        The best tools for VGC TO and Judges
    </description>
    <author email="nataliagtrd@gmail.com" href="http://www.twitter.com/natisp27">
        Natalia Guti√©rrez
    </author>
    <content src="index.html" />
    <preference name="android-minSdkVersion" value="16" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-admob-simple" spec="^3.3.4" />
    <engine name="android" spec="^7.0.0" />
</widget>
