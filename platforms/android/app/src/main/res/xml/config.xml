<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="10001" id="com.natisp.theprofessorbook" version="0.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.cupertino.cordova.plugin.AdMob" />
        <param name="onload" value="true" />
    </feature>
    <name>The Professor Book</name>
    <description>
        The best tools for VGC TO and Judges
    </description>
    <author email="nataliagtrd@gmail.com" href="http://www.twitter.com/natisp27">
        Natalia Guti√©rrez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="16" />
</widget>
