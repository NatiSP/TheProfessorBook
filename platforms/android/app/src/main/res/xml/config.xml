<?xml version='1.0' encoding='utf-8'?>
<widget id="com.natisp.theprofessorbook" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.cupertino.cordova.plugin.AdMob" />
        <param name="onload" value="true" />
    </feature>
    <name>The Professor Book</name>
    <description>
        The best tools for VGC TO and Judges
    </description>
    <author email="nataliagtrd@gmail.com" href="http://www.twitter.com/natisp27">
        Natalia Guti√©rrez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="16" />
    <feature name="InAppBillingV3">
        <param name="android-package" value="com.alexdisler.inapppurchases.InAppBillingV3" />
    </feature>
</widget>
